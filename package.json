{
  "name": "WLAN_Key_Revealer",
  "version": "1.0.0",
  "description": "Shows WLAN Keys in Cleartext",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=GriepMarco --version-string.FileDescription=CE --version-string.ProductName=\"WLAN Key Revealer\"",    
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=linux/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": "",
  "keywords": [
    "WLAN"
  ],
  "author": "Griep Marco",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^4.0.7",
    "electron-packager": "^13.1.1"
  },
  "dependencies": {
    "jquery": "^3.3.1"
  }
}
 